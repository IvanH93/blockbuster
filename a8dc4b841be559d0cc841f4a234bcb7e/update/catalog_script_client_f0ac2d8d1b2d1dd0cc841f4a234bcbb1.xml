        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {
    if (isLoading || newValue == '') {
        return;
    }
    var grAjax = new GlideAjax("prenota_film_o_videogame_utils");
    grAjax.addParam("sysparm_name", "getCostAndReturnTime");
    grAjax.addParam("sysparm_prodottoID", g_form.getValue("prodotto"));
    grAjax.getXML(getCosto);

    function getCosto(response) {
        var result = JSON.parse(response.responseXML.documentElement.getAttribute("answer"));
		
        g_form.setValue("costo", result.costo);
        g_form.setValue("data_restituzione", result.data_restituzione);

    }


}]]></script>
